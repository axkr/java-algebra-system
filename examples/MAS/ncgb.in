(* Non-commuative Groebner bases. *)

(* Commutator relations: *)
t:=PREAD().
(a,x,y) L
(
( y ), ( x ), ( x y + a ), 
)
PWRITE(t). 

(* Non-commutative polynomials: *)
p:=NPREAD(t).
(
( y**3 + x**2 y + x y ),
( x**2 + x )
)
PWRITE(p).

(* Left Normalform: *)
tp:=t.
c:=LIRRSET(tp,p). 
PWRITE(c).


(* Left G-base: *)
tp:=t.
c:=LGBASE(tp,p,1). 
PWRITE(c).


(* Two sided G-base: *)
tp:=t.
c:=TSGBASE(tp,p,1). 
PWRITE(c).

(* -EOF- *)
