(* James H. Davenport and Joos Heintz. 
   Real Quantifier Elimination is Doubly Exponential.
   Journal of Symbolic Computaion, 5 ( 1 & 2):29-35; February 1988
*)

IN("example.in").
{VALIS=(a,b,c,d)}
(  ex c: fa b,a : (( 
	([(a) = (d) ] /\ [ (b) = (c)]) \/
	([(a) = (c) ] /\ [ (b) = (1)]))
	=> 
	[ (a**2) = (b)] )
) . 
EXIT.
