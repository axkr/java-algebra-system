(* Katsura, Laurent Series Case 4, 18. Feb. 1985
   from: Boege, W., Gebauer, R., Kredel, H.,
         Some Examples for Solving Systems of Algebraic Equations
         by Calculating Groebner Bases,
         J. Symbolic Computation (1986) 1, 83-96. *)


   OUT("Katsura4.out").

   SWRITE("Example from BGK: Katsura, Laurent Series Case 4, 18. Feb. 1985").

   VariableList := LIST("U0","U1","U2","U3","U4").
   V := DIPVDEF(VariableList).
   TermOrder := DIPTODEF(8).

   SetDIPAGBTraceFlag(2).

   SWRITE("Groebner basis calculations over INTEGER domain:").

      DomainDescriptor := ADDDREAD(). INT.
      F := DILRD(VariableList,DomainDescriptor).

      ( ( U0**2 - U0 + 2 U1**2 + 2 U2**2 + 2 U3**2 + 2 U4**2 )
        ( 2 U0 U1 + 2 U1 U2 + 2 U2 U3 + 2 U3 U4 - U1 )
        ( 2 U0 U2 + U1**2 + 2 U1 U3 + 2 U2 U4 - U2 )
        ( 2 U0 U3 + 2 U1 U2 + 2 U1 U4 - U3 )
        ( U0 + 2 U1 + 2 U2 + 2 U3 + 2 U4 - 1 ) )

      DILWR(F,VariableList).

      (* Groebner basis computation with DIPAGB and normal strategy: *)

         SetDIPAGBStrategy(0).
         WriteDIPAGBOptions.
         DILWR(DIPAGB(F),VariableList).

      (* Groebner basis computation with DIPAGB, normal with sugar strategy
         and variable weight list (0,0,0,0): *)

         SetDIPAGBStrategy(1).
         SetDIPAGBVariableWeight(LIST(0,0,0,0,0)).
         WriteDIPAGBOptions.
         DILWR(DIPAGB(F),VariableList).

      (* Groebner basis computation with DIPAGB, normal with sugar strategy
         and variable weight list (1,1,1,1): *)

         r:=RNINT(1).
         SetDIPAGBVariableWeight(LIST(r,r,r,r)).
         WriteDIPAGBOptions.
         DILWR(DIPAGB(F),VariableList).

      (* Groebner basis computation with DIIFGB: *)

         SWRITE("Groebner Basis Computation with DIIFGB:").
         DILWR(DIIFGB(F,1),VariableList).

   SWRITE("Groebner basis calculations over RATIONAL NUMBER domain:").

      DomainDescriptor := ADDDREAD(). RN.
      F := DILRD(VariableList,DomainDescriptor).

      ( ( U0**2 - U0 + 2 U1**2 + 2 U2**2 + 2 U3**2 + 2 U4**2 )
        ( 2 U0 U1 + 2 U1 U2 + 2 U2 U3 + 2 U3 U4 - U1 )
        ( 2 U0 U2 + U1**2 + 2 U1 U3 + 2 U2 U4 - U2 )
        ( 2 U0 U3 + 2 U1 U2 + 2 U1 U4 - U3 )
        ( U0 + 2 U1 + 2 U2 + 2 U3 + 2 U4 - 1 ) )

      DILWR(F,VariableList).

      (* Groebner basis computation with DIPAGB and normal strategy: *)

         SetDIPAGBStrategy(0).
         WriteDIPAGBOptions.
         DILWR(DIPAGB(F),VariableList).

      (* Groebner basis computation with DIPAGB, normal with sugar strategy
         and variable weight list (0,0,0,0): *)

         SetDIPAGBStrategy(1).
         SetDIPAGBVariableWeight(LIST(0,0,0,0,0)).
         WriteDIPAGBOptions.
         DILWR(DIPAGB(F),VariableList).

      (* Groebner basis computation with DIPAGB, normal with sugar strategy
         and variable weight list (1,1,1,1): *)

         r:=RNINT(1).
         SetDIPAGBVariableWeight(LIST(r,r,r,r)).
         WriteDIPAGBOptions.
         DILWR(DIPAGB(F),VariableList).

      (* Groebner basis computation with DIPGB: *)

         SWRITE("Groebner Basis Computation with DIPGB:").
         DILWR(DIPGB(F,1),VariableList).

   SHUT("Katsura4.out").
   EXIT.

(* -EOF- *)