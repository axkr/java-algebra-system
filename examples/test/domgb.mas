CLOUT("Computing Groebner base ... "). 

(*Modular Digit ----------------------------- *) 
(*domain descriptor         <modulus> *) 
dp:=ADDDREAD().          MD 17
 
ADDDWRIT(dp).
 
(*variable list *)
V:=DIPVDEF(LIST("B","S","T","Z","P","W")). 
 
(*Polynomials *)
P:=DILRD(V,dp).
 
( 
( 45 P + 35 S - 165 B - 36 ), 
( 35 P + 40 Z + 25 T - 27 S ), 
( 15 W + 25 S P + 30 Z - 18 T - 165 B**2 ), 
( - 9 W + 15 T P + 20 S Z ), 
( P W + 2 T Z - 11 B**3 ), 
( 99 W - 11 B S + 3 B**2 ) 
) 

(*Syntax: <polynomial list> *) 
(* ( B**2 + 33/50 B + 2673/10000 ) *) 

DILWR(P,V).
 
(* computations *)
Q:=DIPGB(P,1). 
DILWR(Q,V).


(*Rational Function ----------------------- *) 
(*domain descriptor        <var list> *) 
dp:=ADDDREAD().          RF(A1,A2,A3,A4)
 
ADDDWRIT(dp).
  
(*variable list 1 und 2 *) 
V:=DIPVDEF(LIST("X1","X2","X3","X4")). 
 
(*Polynomials *)
P:=DILRD(V,dp).
 
(
( X4 - ( A4 - A2 )),
( X1 + X2 + X3 + X4 - ( A1 + A3 + A4 )),
( X1 X3 + X1 X4 + X2 X3 + X3 X4 - ( A1 A4 + A1 A3 + A3 A4 )),
( X1 X3 X4 - ( A1 A3 A4 ))
)

DILWR(P,V).    

(* computations *)
Q:=DIPGB(P,1). 
DILWR(Q,V).
 
(* -EOF- *)
