IN("example.in").
{VALIS=(a3,a2,a1,a0,b1,b0,c2,c1,c0,x)}
( fa a3,a2,a1,a0 ( 
  ex b1, b0, c2, c1, c0 (
    [ ( a3 x**3 + a2 x**2 + a1 x + a0  - 
       b0 c0 - b0 c1 x - b0 c2 x**2 - b1 c0 x - b1 c1 x**2 - b1 c2 x**3 ) = 0 ]
))). 
EXIT.
