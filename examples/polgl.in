CLOUT("Computing Groebner base ... "). 
  
 P:=PREAD(). 
 
(a0,a1,a2,b1,b2,c0,c1,c2) L 

(
( a2 a2 + b2 b2 - c2 c2 ),
( a1 a2 + b1 b2 - c1 c2 ),
( 2 a0 a2 + a1 a1 + b1 b1 - 2 c0 c2 - c1 c1) ,
( a0 a1 - c0 c1 ),
( a0 a0 - c0 c0 )
)

(*Syntax: <variable list> <term order> 
          <polynomial list> *) 
  
PWRITE(P). 
 
Q:=DIRPGB(P,1). 
 
PWRITE(Q). 
 
